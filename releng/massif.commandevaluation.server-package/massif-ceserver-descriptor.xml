<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.1.0 http://maven.apache.org/xsd/assembly-2.1.0.xsd">
	<id>massif-ce-server</id>
	<baseDirectory>massif-ce-server</baseDirectory>
	<formats>
		<format>zip</format>
	</formats>
	<dependencySets>
		<dependencySet>
			<excludes>
				<exclude>hu.bme.mit.massif:br.com.embraer.massif.commandevaluation.tests</exclude>
			</excludes>
			<outputDirectory></outputDirectory>
			<useProjectArtifact>true</useProjectArtifact>
			<outputFileNameMapping>${artifact.artifactId}.${artifact.extension}</outputFileNameMapping>
			<scope>runtime</scope>
			<useTransitiveDependencies>false</useTransitiveDependencies>
		</dependencySet>
	</dependencySets>
	<moduleSets>
		<moduleSet>
			<useAllReactorProjects>true</useAllReactorProjects>
			<includes>
				<include>hu.bme.mit.massif:br.com.embraer.massif.commandevaluation.server</include>
			</includes>
			<sources>
				<includeModuleDirectory>false</includeModuleDirectory>
				<fileSets>
					<fileSet>
						<directory></directory>
						<includes>
							<include>setup_remote.m</include>
						</includes>
					</fileSet>
				</fileSets>
			</sources>
		</moduleSet>
		<moduleSet>
			<useAllReactorProjects>true</useAllReactorProjects>
			<includes>
				<include>hu.bme.mit.massif:hu.bme.mit.massif.simulink.api</include>
			</includes>
			<sources>
				<includeModuleDirectory>false</includeModuleDirectory>
				<fileSets>
					<fileSet>
						<directory>scripts</directory>
						<includes>
							<include>massif_functions.m</include>
						</includes>
					</fileSet>
				</fileSets>
			</sources>
		</moduleSet>
		<moduleSet>
			<useAllReactorProjects>true</useAllReactorProjects>
			<includes>
				<include>hu.bme.mit.massif:br.com.embraer.massif.commandevaluation.tests</include>
			</includes>
			<sources>
				<includeModuleDirectory>false</includeModuleDirectory>
				<fileSets>
					<fileSet>
						<directory>testconfig</directory>
						<includes>
							<include>matlabserverconfig.properties</include>
						</includes>
					</fileSet>
				</fileSets>
			</sources>
		</moduleSet>
	</moduleSets>
</assembly>